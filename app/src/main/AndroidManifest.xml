<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tonio.rentallmanager" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Menu_Activity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Cargar"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".Alquilar"
            android:label="@string/title_activity_alquilar"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".listaCabanias"
            android:label="@string/title_activity_lista_cabanias"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".Editar"
            android:label="@string/title_activity_editar"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
    </application>

</manifest>
